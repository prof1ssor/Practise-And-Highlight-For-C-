#include <iostream>
#include <limits>
using namespace std;

int main()
{
    cout << "The size of long is " << sizeof(1L) << "\n";
    cout << "The size of long long is " << sizeof(1LL) << "\n";

    cout << "Large float == " << numeric_limits<float>::max() << "\n";
    cout << "Char is signed == " << numeric_limits<char>::is_signed << "\n";

    return 0;
}
